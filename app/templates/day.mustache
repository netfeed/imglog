<div id="images">
  <div class="toprow">
    <div class="previous">
      {{#previous}}
        <a href="{{date_path}}">&lt;&lt; {{created_date}}</a>
      {{/previous}}
    </div>

    <div class="middle">
      {{#middle}}
        <a href="{{date_path}}">{{pretty_date}}</a>
      {{/middle}}
    </div>

    <div class="next">
      {{#next}}
        <a href="{{date_path}}">{{created_date}} &gt;&gt;</a>
      {{/next}}
    </div>
  </div>

  <h1>{{date}} ({{amount}} images)</h1>
  {{#images}}
    <div class="row">
      {{#row}}
        <div class="thumbnail"><a href="{{url}}"><img src="{{thumbnail}}"/></a></div>
      {{/row}}
    </div>
  {{/images}}
</div>